}

.factories .dragger-scrollbar {
    width: 100%;
    margin-right: 0
}

@media (max-width:47.99em) {
    .factories .factories-footer .dragger-controls,
    .factories .factories-slide-link {
        margin-top: 30px
    }
}


@media (max-width:47.99em) {
    .factories .dragger-footer {
        margin-top: 0
    }
}

.factories .dragger-footer .dragger-scrollbar-handle {
    transition: -webkit-transform 1s cubic-bezier(.19, 1, .22, 1);
    transition: transform 1s cubic-bezier(.19, 1, .22, 1);
    transition: transform 1s cubic-bezier(.19, 1, .22, 1), -webkit-transform 1s cubic-bezier(.19, 1, .22, 1)
}

[data-router-view=home] .dragger {
    margin-top: 190px;
    margin-bottom: 190px
}

@media (max-width:47.99em) {
    [data-router-view=home] .dragger {
        margin-top: 90px;
        margin-bottom: 90px
    }
}

.single-product .dragger {
    margin-bottom: 150px;
    margin-top: 150px
}

@media (max-width:47.99em) {
    .single-product .dragger {
        margin-top: 90px;
        margin-bottom: 90px
    }
}
[data-router-view=capsules] .header img {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%
}

[data-router-view=page] {
    margin-top: 200px
}

[data-router-view=page] a {
    color: #dbb4ae;
    text-decoration: none
}

[data-router-view=page] h1 {
    margin-bottom: 30px
}

[data-router-view=page] .terms h1,
[data-router-view=page] .terms h2,
[data-router-view=page] .terms h3 {
    margin-top: 0!important
}

[data-router-view=page] .terms .article {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin-bottom: 100px
}

[data-router-view=page] .terms .article .large-4 {
    height: 31.5315315315%;
    width: 31.5315315315%
}

@media (max-width:47.99em) {
    [data-router-view=page] .terms .article .large-4 {
        width: 100%
    }
}

[data-router-view=page] .terms .article .large-8 {
    height: 57.2072072072%;
    width: 57.2072072072%;
    margin-left: 11.2612612613%
}

@media (max-width:47.99em) {
    [data-router-view=page] .terms .article .large-8 {
        width: 100%;
        margin: 0
    }
}

[data-router-view=page] h1 {
    margin-top: 40px
}

[data-router-view=page] h2,
[data-router-view=page] h3 {
    margin-bottom: 20px;
    margin-top: 60px
}

[data-router-view=page] h4 {
    margin-bottom: 10px;
    margin-top: 30px;
    font-weight: 700
}
body {
    overflow-x: hidden; /* Hide horizontal scrollbar */
}


