<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="styles"/>
    <body>
        <referenceContainer name="left">
            <block class="Unirgy\SimpleUp\Block\Adminhtml\Module\Tabs"
                   name="usimpleup.module.tabs"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block name="usimpleup.module" class="Unirgy\SimpleUp\Block\Adminhtml\Module">
                <container name="usimpleup.module.tabs.container" label="Tabs"></container>
            </block>
        </referenceContainer>
    </body>
</page>
