    </type>
    <type name="ShipperHQ\Shipper\Helper\CarrierCache">
        <plugin name="SomethingDigital_ShipperResult" type="SomethingDigital\ShipperHqCustomizations\Plugin\SaveDatesFromCache" />
    </type>
    <type name="Magento\Checkout\Model\DefaultConfigProvider">
        <plugin name="checkout-summary-product-deliveryinfo" type="SomethingDigital\ShipperHqCustomizations\Plugin\Checkout\Model\DefaultConfigProviderPlugin" />
    </type>
    <preference for="Magento\Quote\Model\Cart\Totals\Item"
