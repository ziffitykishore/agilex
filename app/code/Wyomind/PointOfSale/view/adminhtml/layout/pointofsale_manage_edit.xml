<?xml version="1.0"?><page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">    <update handle="editor"/>    <head>        <css src="Wyomind_PointOfSale::css/edit.css"/>    </head>    <body>        <referenceContainer name="content">            <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit\Template" template="Wyomind_PointOfSale::edit/gmap.phtml" name="google-map"></block>            <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit" name="pointofsale_manage_edit"/>        </referenceContainer>            <referenceContainer name="left">            <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit\Tabs" name="pointofsale_manage_edit_tabs">                <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit\Tab\Main" name="pointofsale_manage_edit_tab_main"/>                <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit\Tab\Address" name="pointofsale_manage_edit_tab_address"/>                <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit\Tab\Storeviews" name="pointofsale_manage_edit_tab_storeviews"/>                <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit\Tab\Customergroups" name="pointofsale_manage_edit_tab_customergroups"/>                <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit\Tab\Frontend" name="pointofsale_manage_edit_tab_frontend"/>                <block class="Wyomind\PointOfSale\Block\Adminhtml\Manage\Edit\Tab\Additional" name="pointofsale_manage_edit_tab_additional"/>                <action method="addTab">                    <argument name="name" xsi:type="string">main_section</argument>                    <argument name="block" xsi:type="string">pointofsale_manage_edit_tab_main</argument>                </action>                <action method="addTab">                    <argument name="name" xsi:type="string">address_section</argument>                    <argument name="block" xsi:type="string">pointofsale_manage_edit_tab_address</argument>                </action>                <action method="addTab">                    <argument name="name" xsi:type="string">storeviews_section</argument>                    <argument name="block" xsi:type="string">pointofsale_manage_edit_tab_storeviews</argument>                </action>                <action method="addTab">                    <argument name="name" xsi:type="string">customergroups_section</argument>                    <argument name="block" xsi:type="string">pointofsale_manage_edit_tab_customergroups</argument>                </action>                <action method="addTab">                    <argument name="name" xsi:type="string">frontend_section</argument>                    <argument name="block" xsi:type="string">pointofsale_manage_edit_tab_frontend</argument>                </action>                <action method="setActiveTab">                    <argument name="name" xsi:type="string">main_section</argument>                </action>                <action method="addTabAfter">                    <argument name="name" xsi:type="string">additional_section</argument>                    <argument name="block" xsi:type="string">pointofsale_manage_edit_tab_additional</argument>                    <argument name="after" xsi:type="string">inventory</argument>                </action>            </block>        </referenceContainer>    </body></page>