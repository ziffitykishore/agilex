<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Sales::sales" title="Sales" sortOrder="20">
                    <resource id="Ziffity_Pickupdate::pickupdate" title="Ziffity: Pickup Date" sortOrder="30" >
                        <resource id="Ziffity_Pickupdate::pickupdate_holidays" title="Exceptions: Working Days and Holidays" sortOrder="10" />
