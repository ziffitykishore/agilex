<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Mageplaza_QuickOrder::css/source/quickorder.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\QuickOrder\Block\Dashboard" name="mp.quickorder.dashboard" template="Mageplaza_QuickOrder::dashboard.phtml" ifconfig="quickorder/general/enabled">
                <block class="Magento\Framework\View\Element\Template" name="mp.quickorder.searchtool" template="Mageplaza_QuickOrder::searchtool.phtml"/>
                <block class="Magento\Framework\View\Element\Template" name="mp.quickorder.design" template="Mageplaza_QuickOrder::design.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>
