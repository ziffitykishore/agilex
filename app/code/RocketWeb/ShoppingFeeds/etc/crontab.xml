<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="rocket_shopping_feeds_schedule" instance="RocketWeb\ShoppingFeeds\Cron\Schedule" method="execute">
            <schedule>10 * * * *</schedule>
        </job>
        <job name="rocket_shopping_feeds_process" instance="RocketWeb\ShoppingFeeds\Cron\Process" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
