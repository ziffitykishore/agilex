{
    "name": "magento/project-enterprise-edition",
    "description": "eCommerce Platform for Growth (Enterprise Edition)",
    "type": "project",
    "version": "2.3.1",
    "license": [
        "proprietary"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/magento2-theme-blank-sass.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/magento2-frontools.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/SomethingDigital-Magento2-Theme-BryantPark.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_Migration.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_Bluefoot.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/composer-patcher.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_AdminNotify.git"
        },
        {
            "type": "git",
            "url": "https://github.com/fastly/fastly-magento2.git"
        },
        {
            "type": "composer",
            "url": "https://packages.mageworx.com/"
        }
    ],
    "require": {
        "php": "~7.1.3||~7.2.0",
        "magento/magento-cloud-metapackage": "2.3.1",
        "composer/composer": "@alpha",
        "snowdog/frontools": "dev-develop#a88aa6",
        "somethingdigital/magento2-theme-bryantpark": "dev-integration/magento-v2.3.1",
        "somethingdigital/magento2-migration": "dev-nobluefoot",
        "jamescowie/composer-patcher": "1.0.3",
        "somethingdigital/module-admin-notify": "^1.0",
        "msp/adminrestriction": "^1.3",
        "ethanyehuda/magento2-cronjobmanager": "^1.6",
        "pelago/emogrifier": "~2.0.0||>=2.1.2",
        "snowdog/theme-blank-sass": "1.2.0",
        "fastly/magento2": "^1.2",
        "magento/extension-b2b": "^1.0",
        "magento/sample-data-media": "100.3.*",
        "magento/module-catalog-sample-data": "100.3.*",
        "magento/module-configurable-sample-data": "100.3.*",
        "magento/module-bundle": "^100.3",
        "magento/module-grouped-product-sample-data": "100.3.*",
        "magento/module-gift-card-sample-data": "100.3.*",
        "magento/module-product-links-sample-data": "100.3.*",
        "magento/module-review-sample-data": "100.3.*",
        "magento/module-swatches-sample-data": "100.3.*",
        "magento/module-bundle-sample-data": "100.3.*",
        "magento/module-customer-sample-data": "100.3.*",
        "magento/module-sales-sample-data": "100.3.*",
        "magento/module-cms-sample-data": "100.3.*",
        "magento/module-theme-sample-data": "100.3.*",
        "magento/module-widget-sample-data": "100.3.*",
        "magento/module-catalog-rule-sample-data": "100.3.*",
        "magento/module-sales-rule-sample-data": "100.3.*",
        "magento/module-downloadable-sample-data": "100.3.*",
        "magento/module-wishlist-sample-data": "100.3.*",
        "magento/module-tax-sample-data": "100.3.*",
        "magento/module-msrp-sample-data": "100.3.*",
        "magento/module-customer-balance-sample-data": "100.3.*",
        "magento/module-offline-shipping-sample-data": "100.3.*",
        "magento/module-gift-registry-sample-data": "100.3.*",
        "magento/module-target-rule-sample-data": "100.3.*",
        "magento/module-multiple-wishlist-sample-data": "100.3.*",
        "algolia/algoliasearch-magento-2": "dev-develop",
        "yotpo/module-review": "^2.4",
        "mageworx/module-sitemapsuitemeta": "^2.3",
        "mageworx/module-xmlsitemap": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.0",
        "squizlabs/php_codesniffer": "3.3.1",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.5.2",
        "friendsofphp/php-cs-fixer": "~2.13.0",
        "lusitanian/oauth": "~0.8.10",
        "sebastian/phpcpd": "~3.0.0",
        "magento/magento2-functional-testing-framework": "~2.3.13",
        "allure-framework/allure-phpunit": "~1.2.0"
    },
    "config": {
        "use-include-path": true
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\ToolkitFramework\\": "dev/tools/performance-toolkit/framework/Magento/ToolkitFramework/"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": "Inviqa\\Command::patch",
        "post-update-cmd": "Inviqa\\Command::patch"
    },
    "extra": {
        "magento-force": "override",
        "patches": {}
    }
}