<sections>
    <general>
        <class>\Magento\Ui\Test\Block\Adminhtml\Section</class>
        <selector>[data-index="general"]</selector>
        <strategy>css selector</strategy>
        <fields>
            <company_name>
                <selector>[name="general[company_name]"]</selector>
            </company_name>
            <status>
                <selector>[name="general[status]"]</selector>
            </status>
            <company_email>
                <selector>[name="general[company_email]"]</selector>
            </company_email>
            <sales_representative_id>
                <selector>[name="general[sales_representative_id]"]</selector>
                <input>select</input>
            </sales_representative_id>
        </fields>
    </general>
    <information>
        <class>\Magento\Ui\Test\Block\Adminhtml\Section</class>
        <selector>[data-index="information"]</selector>
        <strategy>css selector</strategy>
        <fields>
            <legal_name>
                <selector>[name="information[legal_name]"]</selector>
            </legal_name>
            <vat_tax_id>
                <selector>[name="information[vat_tax_id]"]</selector>
            </vat_tax_id>
            <reseller_id>
                <selector>[name="information[reseller_id]"]</selector>
            </reseller_id>
            <comment>
                <selector>[name="information[comment]"]</selector>
            </comment>
        </fields>
    </information>
    <address>
        <class>\Magento\Ui\Test\Block\Adminhtml\Section</class>
        <selector>[data-index="address"]</selector>
