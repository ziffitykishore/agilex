<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Config\Test\Repository\ConfigData">
        <dataset name="company_enable">
            <field name="btob/website_configuration/company_active" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
        </dataset>
        <dataset name="company_enable_rollback">
            <field name="btob/website_configuration/company_active" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="company_registration_storefront_disable">
            <field name="company/general/allow_company_registration" xsi:type="array">
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="company_registration_storefront_disable_rollback">
            <field name="company/general/allow_company_registration" xsi:type="array">
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
        </dataset>
    </repository>
</config>
