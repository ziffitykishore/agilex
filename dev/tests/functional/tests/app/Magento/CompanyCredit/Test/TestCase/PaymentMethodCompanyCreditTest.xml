            <constraint name="Magento\CompanyCredit\Test\Constraint\AssertCompanyCreditBalance" />
            <constraint name="Magento\CompanyCredit\Test\Constraint\AssertCompanyCreditBalanceHistoryOperations" />
        </variation>
