<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/pages.xsd">
    <page name="SharedCatalogConfigure" area="Adminhtml" mca="shared_catalog/sharedCatalog/wizard/" module="Magento_SharedCatalog">
        <block name="wizard" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\Wizard" locator="aside[data-type='slide']" strategy="css selector"/>
        <block name="container" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\Container" locator="#container" strategy="css selector"/>
        <block name="stateJstree" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\Tree" locator=".shared-catalog-configure-state" strategy="css selector"/>
        <block name="navigation" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\Navigation" locator=".steps-wizard-navigation" strategy="css selector"/>
        <block name="structureGrid" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\StructureGrid" locator="#catalog-steps-wizard_step_structure .admin__data-grid-outer-wrap" strategy="css selector"/>
        <block name="structureJstree" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\Tree" locator="#catalog-steps-wizard_step_structure div.configure-step-left" strategy="css selector"/>
        <block name="pricingGrid" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\PricingGrid" locator="#catalog-steps-wizard_step_pricing .admin__data-grid-outer-wrap" strategy="css selector"/>
        <block name="tierPriceModal" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\TierPriceModal" locator=".modal-slide.tier_price_modal" strategy="css selector"/>
        <block name="pricingJstree" class="Magento\SharedCatalog\Test\Block\Adminhtml\Wizard\Tree" locator="#catalog-steps-wizard_step_pricing div.configure-step-left" strategy="css selector"/>
        <block name="discount" class="Magento\SharedCatalog\Test\Block\Adminhtml\Popup" locator=".modal-popup.prompt" strategy="css selector"/>
        <block name="pageActionBlock" class="Magento\Backend\Test\Block\FormPageActions" locator=".page-main-actions" strategy="css selector"/>
    </page>
</config>
