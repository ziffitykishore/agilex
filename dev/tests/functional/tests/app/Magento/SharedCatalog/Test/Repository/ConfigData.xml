<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Config\Test\Repository\ConfigData">
        <dataset name="category_permissions_on">
            <field name="catalog/magento_catalogpermissions/enabled" xsi:type="array">
                <item name="scope" xsi:type="string">catalog</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="catalog/magento_catalogpermissions/grant_catalog_product_price" xsi:type="array">
                <item name="scope" xsi:type="string">default</item>
                <item name="scope_id" xsi:type="number">0</item>
                <item name="label" xsi:type="string">Yes, for Everyone</item>
                <item name="value" xsi:type="number">1</item>
            </field>
            <field name="catalog/magento_catalogpermissions/grant_checkout_items" xsi:type="array">
                <item name="scope" xsi:type="string">default</item>
                <item name="scope_id" xsi:type="number">0</item>
                <item name="label" xsi:type="string">Yes, for Everyone</item>
                <item name="value" xsi:type="number">1</item>
            </field>
        </dataset>
        <dataset name="category_permissions_on_rollback">
            <field name="catalog/magento_catalogpermissions/enabled" xsi:type="array">
                <item name="scope" xsi:type="string">catalog</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="category_permissions_off">
            <field name="catalog/magento_catalogpermissions/enabled" xsi:type="array">
                <item name="scope" xsi:type="string">catalog</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="category_permissions_off_rollback">
            <field name="catalog/magento_catalogpermissions/enabled" xsi:type="array">
                <item name="scope" xsi:type="string">catalog</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="shared_catalog_enable">
            <field name="btob/website_configuration/sharedcatalog_active" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Yes</item>
                <item name="value" xsi:type="number">1</item>
            </field>
        </dataset>
        <dataset name="shared_catalog_enable_rollback">
            <field name="btob/website_configuration/sharedcatalog_active" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="shared_catalog_disable">
            <field name="btob/website_configuration/sharedcatalog_active" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="shared_catalog_disable_rollback">
            <field name="btob/website_configuration/sharedcatalog_active" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">No</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="customer_account_share_on">
            <field name="customer/account_share/scope" xsi:type="array">
                <item name="scope" xsi:type="string">customer</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Global</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="customer_account_share_on_rollback">
            <field name="customer/account_share/scope" xsi:type="array">
                <item name="scope" xsi:type="string">customer</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Per Website</item>
                <item name="value" xsi:type="number">1</item>
            </field>
        </dataset>
    </repository>
</config>
