<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../vendor/magento/mtf/etc/pages.xsd">
    <page name="NegotiableQuoteEdit" area="Adminhtml" mca="quotes/quote/view" module="Magento_NegotiableQuote">
        <block name="quoteDetails" class="Magento\NegotiableQuote\Test\Block\Adminhtml\QuoteDetails" locator="[id='page:main-container']" strategy="css selector"/>
        <block name="quoteDetailsActions" class="Magento\NegotiableQuote\Test\Block\Adminhtml\QuoteDetailsActions" locator=".page-main-actions" strategy="css selector"/>
        <block name="quoteDeclinePopup" class="Magento\NegotiableQuote\Test\Block\Adminhtml\QuoteDeclinePopup" locator=".popup-tree-decline" strategy="css selector"/>
        <block name="quoteDeclineRestrictionPopup" class="Magento\NegotiableQuote\Test\Block\Adminhtml\QuoteDeclineRestrictionPopup" locator=".restriction-modal-quote._show" strategy="css selector"/>
        <block name="quoteConfigurablePopup" class="Magento\NegotiableQuote\Test\Block\Adminhtml\QuoteConfigurablePopup" locator=".configurable-negotiable-modal" strategy="css selector"/>
        <block name="quoteShippingInformation" class="Magento\NegotiableQuote\Test\Block\Adminhtml\QuoteShippingInformation" locator="#shipping-information" strategy="css selector"/>
        <block name="quoteMessages" class="Magento\NegotiableQuote\Test\Block\Adminhtml\QuoteMessages" locator="[data-role='message-block']" strategy="css selector"/>
        <block name="systemMessages" class="Magento\NegotiableQuote\Test\Block\Adminhtml\SystemMessages" locator="#system_messages" strategy="css selector" />
    </page>
</config>
