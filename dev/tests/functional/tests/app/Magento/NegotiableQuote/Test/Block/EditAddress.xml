<mapping strict="0">
    <fields>
        <firstname>
            <selector>[name="address[firstname]"]</selector>
        </firstname>
        <lastname>
            <selector>[name="address[lastname]"]</selector>
        </lastname>
        <city>
            <selector>[name="address[city]"]</selector>
        </city>
        <street>
            <selector>[name="address[street][0]"]</selector>
        </street>
        <company>
            <selector>[name="address[company]"]</selector>
        </company>
        <country_id>
            <selector>[name="address[country_id]"]</selector>
            <input>select</input>
        </country_id>
        <region_id>
            <selector>[name="address[region_id]"]</selector>
            <input>select</input>
        </region_id>
        <postcode>
            <selector>[name="address[postcode]"]</selector>
        </postcode>
        <telephone>
            <selector>[name="address[telephone]"]</selector>
        </telephone>
    </fields>
</mapping>
