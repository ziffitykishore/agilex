<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Config\Test\Repository\ConfigData">
        <dataset name="specific_payment_methods">
            <field name="btob/website_configuration/default_b2b_payment_methods/applicable_payment_methods" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Specific Payment Methods</item>
                <item name="value" xsi:type="number">1</item>
            </field>
        </dataset>
        <dataset name="specific_payment_methods_rollback">
            <field name="btob/website_configuration/default_b2b_payment_methods/applicable_payment_methods" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">All Enabled Payment Methods</item>
                <item name="value" xsi:type="number">0</item>
            </field>
        </dataset>
        <dataset name="select_specific_payment_methods">
            <field name="btob/website_configuration/default_b2b_payment_methods/available_payment_methods" xsi:type="array">
                <item name="scope" xsi:type="string">b2b</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="array">
                    <item name="Cash On Delivery" xsi:type="string">cashondelivery</item>
                    <item name="Payment on Account" xsi:type="string">companycredit</item>
                </item>
            </field>
        </dataset>
    </repository>
</config>
