    "lusitanian/oauth": "~0.8.10",
    "sebastian/phpcpd": "2.0.4",
    "mikey179/vfsStream": "^1.6",
    "phpro/grumphp": "^0.12.1",
    "theseer/phpdox": "^0.10.1",
    "phploc/phploc": "^4.0",
    "squizlabs/php_codesniffer": "^3.2",
