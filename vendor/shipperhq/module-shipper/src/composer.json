{
  "name": "shipperhq/module-shipper",
  "description": "Magento Shipping integration with ShipperHQ",
  "type": "magento2-module",
  "version": "20.20.1",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "authors": [
    {
      "name": "ShipperHQ",
      "email": "sales@shipperhq.com",
      "homepage": "http://www.Shipperhq.com"
    },
    {
      "name": "Zowta LLC"
    }
  ],
  "support": {
    "documentation": "http://docs.shipperhq.com"
  },
  "require": {
    "magento/framework": ">=100.0",
    "magento/module-store": ">=100.0",
    "shipperhq/library-ws": "20.3.*",
    "shipperhq/library-shipper": "20.6.*",
    "shipperhq/module-common": "20.6.*",
    "shipperhq/module-logger": "*"
